eval `{golang tool dist env -9}

if(! test -x $GOTOOLDIR/dist){
	echo 'cannot find $GOTOOLDIR/dist; nothing to clean' >[1=2]
	exit noclean
}

$GOBIN/golang clean -i std
$GOBIN/golang tool dist clean
$GOBIN/golang clean -i cmd
